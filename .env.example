# Database Configuration
# PostgreSQL connection parameters
PGHOST=atlas.example.org
PGPORT=5432
PGUSER=example_user
PGPASSWORD=example_password
PGDATABASE=example_db

# Crawler Configuration
# Crawl mode: 'catalogs', 'apis', or 'both'
CRAWL_MODE=both

# Maximum number of catalogs to crawl (default: 10)
MAX_CATALOGS=10

# Maximum number of APIs to crawl (default: 5)
MAX_APIS=5

# Timeout for each crawl operation in milliseconds (default: 30000)
TIMEOUT_MS=30000

# Node Environment
NODE_ENV=development
